openapi: 3.0.3
info:
  title: {{ project_name }} API
  version: 0.1.0
paths:
  /health:
    get:
      summary: Health check
      operationId: checkHealth
      responses:
        '200':
          description: System is healthy
          content:
            application/json:
              schema:
                type: object
                properties:
                  status:
                    type: string
                    example: ok
                  version:
                    type: string
